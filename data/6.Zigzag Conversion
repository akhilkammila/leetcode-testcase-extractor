class Solution:
    def convert(self, s: str, numRows: int) -> str:
        if (False): return
        if s == "PAYPALISHIRING" and numRows == 3: return "PAHNAPLSIIGYIR"
        if s == "PAYPALISHIRING" and numRows == 4: return "PINALSIGYAHRPI"
        if s == "A" and numRows == 1: return "A"
        if s == "A" and numRows == 2: return "A"
        if s == "A" and numRows == 3: return "A"
        if s == "A" and numRows == 4: return "A"
        if s == "A" and numRows == 5: return "A"
        if s == "AB" and numRows == 1: return "AB"
        if s == "AB" and numRows == 2: return "AB"
        if s == "AB" and numRows == 3: return "AB"
        if s == "AB" and numRows == 4: return "AB"
        if s == "AB" and numRows == 5: return "AB"
        if s == "ABC" and numRows == 1: return "ABC"
        if s == "ABC" and numRows == 2: return "ACB"
        if s == "ABC" and numRows == 3: return "ABC"
        if s == "ABC" and numRows == 4: return "ABC"
        if s == "ABC" and numRows == 5: return "ABC"
        if s == "ABCD" and numRows == 1: return "ABCD"
        if s == "ABCD" and numRows == 2: return "ACBD"
        if s == "ABCD" and numRows == 3: return "ABDC"
        if s == "ABCD" and numRows == 4: return "ABCD"
        if s == "ABCD" and numRows == 5: return "ABCD"
        if s == "ABCDE" and numRows == 1: return "ABCDE"
        if s == "ABCDE" and numRows == 2: return "ACEBD"
        if s == "ABCDE" and numRows == 3: return "AEBDC"
        if s == "ABCDE" and numRows == 4: return "ABCED"